<header><h1>Nothing but Times</h1></header>
<div id="container">
<% for (let i = 0; i < 9; i++) { %>
    
    <div class = "article" id="<%= info.data.results[i].id %>">
        <h2><%= info.data.results[i].title %></h2>

         <p><%= info.data.results[i].byline %></p>

        <p><%= info.data.results[i].published_date %></p>

        <span id="links">
            <a href= "<%= info.data.results[i].url %>" > Go to article</a>
            <form action="favorites" method="GET">
                <input type="hidden" name="title" value="info.data.results[i].title">
                <input type="hidden" name="byline" value="info.data.results[i].byline">
                <input type="hidden" name="publishedDate" value="info.data.results[i].published_date">
                <input type="hidden" name="url" value="info.data.results[i].url">
                <input type="hidden" name="id" value="info.data.results[i].id">
                <button name="favorite" type="input" alt="Save for later." >‚ù§</button>
                </form>
        </span>
     </div>
    
<% } %>
</div>

